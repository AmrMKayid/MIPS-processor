v0x19f8270_0 .net "clk", 0 0, C4<z>; 0 drivers
v0x19f8330_0 .net "test", 0 0, C4<z>; 0 drivers
E_0x19f8240 .event posedge, v0x19f8270_0;
    .scope S_0x19c1fa0;
T_0 ;
    %set/v v0x19f8050_0, 0, 2;
    %set/v v0x19f8190_0, 0, 1;
    %end;
    .thread T_0;
    .scope S_0x19c1fa0;
T_1 ;
    %vpi_call 2 19 "$monitor", "time=%t", $time, "  a=%d", &PV<v0x19f8050_0, 0, 1>, "  b=%d", &PV<v0x19f8050_0, 1, 1>, "  sel=%b", v0x19f8190_0, "\000", "  out:%d", v0x19f8110_0;
    %movi 8, 4, 4;
T_1.0 %cmp/s 0, 8, 4;
    %jmp/0xz T_1.1, 5;
    %add 8, 1, 4;
    %movi 12, 2, 3;
T_1.2 %cmp/s 0, 12, 3;
    %jmp/0xz T_1.3, 5;
    %add 12, 1, 3;
    %delay 5, 0;
    %load/v 15, v0x19f8190_0, 1;
    %mov 16, 0, 31;
    %addi 15, 1, 32;
    %set/v v0x19f8190_0, 15, 1;
    %jmp T_1.2;
T_1.3 ;
    %load/v 12, v0x19f8050_0, 2;
    %mov 14, 0, 30;
    %addi 12, 1, 32;
    %set/v v0x19f8050_0, 12, 2;
    %jmp T_1.0;
T_1.1 ;
    %delay 10, 0;
    %vpi_call 2 26 "$finish";
    %end;
    .thread T_1;
    .scope S_0x19af690;
T_2 ;
    %wait E_0x19f8240;
    %load/v 8, v0x19f8330_0, 1;
    %mov 9, 0, 2;
    %cmpi/u 8, 1, 3;
    %inv 6, 1;
    %jmp/0xz  T_2.0, 6;
    %vpi_call 4 11 "$display", "ASSERTION FAILED in %m";
    %vpi_call 4 12 "$finish";
T_2.0 ;
    %jmp T_2;
    .thread T_2;
