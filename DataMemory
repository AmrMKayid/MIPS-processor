#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1f96970 .scope module, "DataMemory_tb" "DataMemory_tb" 2 8;
 .timescale 0 0;
v0x1fcb920_0 .var "Clk", 0 0;
v0x1fcb9c0_0 .var "MemRead", 0 0;
v0x1fcba70_0 .var "MemWrite", 0 0;
v0x1fcbb20_0 .var "address", 31 0;
v0x1fcbc00_0 .var "data_in", 31 0;
v0x1fcbcb0_0 .net "data_out", 31 0, v0x1fcb7c0_0; 1 drivers
S_0x1f81690 .scope module, "dm" "DataMemory" 2 25, 3 14, S_0x1f96970;
 .timescale 0 0;
v0x1f81780_0 .net "Clk", 0 0, v0x1fcb920_0; 1 drivers
v0x1fcb530_0 .net "MemRead", 0 0, v0x1fcb9c0_0; 1 drivers
v0x1fcb5d0_0 .net "MemWrite", 0 0, v0x1fcba70_0; 1 drivers
v0x1fcb670_0 .net "address", 31 0, v0x1fcbb20_0; 1 drivers
v0x1fcb720_0 .net "data_in", 31 0, v0x1fcbc00_0; 1 drivers
v0x1fcb7c0_0 .var "data_out", 31 0;
v0x1fcb8a0 .array "memory", 0 63, 31 0;
E_0x1f93df0/0 .event edge, v0x1fcb670_0;
E_0x1f93df0/1 .event posedge, v0x1fcb530_0;
E_0x1f93df0 .event/or E_0x1f93df0/0, E_0x1f93df0/1;
E_0x1f94160 .event posedge, v0x1f81780_0;
    .scope S_0x1f81690;
T_0 ;
    %wait E_0x1f94160;
    %load/v 8, v0x1fcb5d0_0, 1;
    %jmp/0xz  T_0.0, 8;
    %load/v 8, v0x1fcb720_0, 32;
    %ix/getv 3, v0x1fcb670_0;
   %jmp/1 t_0, 4;
   %ix/load 1, 0, 0;
   %set/av v0x1fcb8a0, 8, 32;
t_0 ;
T_0.0 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x1f81690;
T_1 ;
    %wait E_0x1f93df0;
    %load/v 8, v0x1fcb530_0, 1;
    %jmp/0xz  T_1.0, 8;
    %ix/getv 3, v0x1fcb670_0;
    %load/av 8, v0x1fcb8a0, 32;
    %set/v v0x1fcb7c0_0, 8, 32;
T_1.0 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1f96970;
T_2 ;
    %set/v v0x1fcb920_0, 0, 1;
T_2.0 ;
    %delay 5, 0;
    %load/v 8, v0x1fcb920_0, 1;
    %inv 8, 1;
    %set/v v0x1fcb920_0, 8, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x1f96970;
T_3 ;
    %vpi_call 2 29 "$monitor", "time=%t", $time, "  In=%d", v0x1fcbc00_0, "  out=%d", v0x1fcbcb0_0, "  clk=%b", v0x1fcb920_0, "  Address:%d", v0x1fcbb20_0;
    %delay 10, 0;
    %set/v v0x1fcba70_0, 1, 1;
    %delay 0, 0;
    %movi 8, 50, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 0, 0;
    %movi 8, 1200, 32;
    %set/v v0x1fcbc00_0, 8, 32;
    %delay 10, 0;
    %movi 8, 63, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 0, 0;
    %movi 8, 5400, 32;
    %set/v v0x1fcbc00_0, 8, 32;
    %delay 15, 0;
    %movi 8, 40, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 0, 0;
    %movi 8, 400, 32;
    %set/v v0x1fcbc00_0, 8, 32;
    %delay 5, 0;
    %set/v v0x1fcba70_0, 0, 1;
    %delay 5, 0;
    %set/v v0x1fcb9c0_0, 1, 1;
    %delay 5, 0;
    %movi 8, 50, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 5, 0;
    %movi 8, 63, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 5, 0;
    %movi 8, 40, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 5, 0;
    %movi 8, 50, 32;
    %set/v v0x1fcbb20_0, 8, 32;
    %delay 5, 0;
    %vpi_call 2 57 "$finish";
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
