T_1 ;
    %set/v v0x1151b70_0, 0, 64;
    %set/v v0x1151ce0_0, 0, 1;
    %end;
    .thread T_1;
    .scope S_0x111adb0;
T_2 ;
    %vpi_call 2 19 "$monitor", "time=%t", $time, "  a=%d", &PV<v0x1151b70_0, 0, 32>, "  b=%d", &PV<v0x1151b70_0, 32, 32>, "  sel=%b", v0x1151ce0_0, "\000", "  out:%d", v0x1151c30_0;
    %movi 8, 16, 6;
T_2.0 %cmp/s 0, 8, 6;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 6;
    %movi 14, 2, 3;
T_2.2 %cmp/s 0, 14, 3;
    %jmp/0xz T_2.3, 5;
    %add 14, 1, 3;
    %delay 5, 0;
    %load/v 17, v0x1151ce0_0, 1;
    %mov 18, 0, 31;
    %addi 17, 1, 32;
    %set/v v0x1151ce0_0, 17, 1;
    %jmp T_2.2;
T_2.3 ;
    %load/v 14, v0x1151b70_0, 64;
    %mov 78, 0, 1;
    %addi 14, 1, 65;
    %set/v v0x1151b70_0, 14, 64;
    %jmp T_2.0;
T_2.1 ;
    %movi 8, 16, 6;
T_2.4 %cmp/s 0, 8, 6;
    %jmp/0xz T_2.5, 5;
    %add 8, 1, 6;
    %movi 14, 2, 3;
T_2.6 %cmp/s 0, 14, 3;
    %jmp/0xz T_2.7, 5;
    %add 14, 1, 3;
    %delay 5, 0;
    %load/v 17, v0x1151ce0_0, 1;
    %mov 18, 0, 31;
    %addi 17, 1, 32;
    %set/v v0x1151ce0_0, 17, 1;
    %jmp T_2.6;
T_2.7 ;
    %ix/load 1, 32, 0;
    %mov 4, 0, 1;
    %jmp/1 T_2.8, 4;
    %load/x1p 47, v0x1151b70_0, 32;
    %jmp T_2.9;
T_2.8 ;
    %mov 47, 2, 32;
T_2.9 ;
    %mov 14, 47, 32; Move signal select into place
    %mov 46, 0, 1;
    %addi 14, 1, 33;
    %ix/load 0, 32, 0;
    %set/x0 v0x1151b70_0, 14, 32;
    %jmp T_2.4;
T_2.5 ;
    %delay 200, 0;
    %vpi_call 2 32 "$finish";
    %end;
    .thread T_2;
    .scope S_0x1119a80;
T_3 ;
